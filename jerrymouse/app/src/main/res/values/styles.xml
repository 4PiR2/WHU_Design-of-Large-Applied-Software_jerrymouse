<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/design_default_color_primary</item>
		<item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
		<item name="colorAccent">@color/colorAccent</item>
	</style>

	<style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@android:color/holo_orange_light</item>
		<item name="colorPrimaryDark">@android:color/holo_orange_dark</item>
		<item name="colorAccent">@android:color/holo_purple</item>
		<item name="android:colorBackground">@color/colo_yellow</item>
		<item name="android:colorForeground">@color/orange</item>
		<item name="android:navigationBarColor">@color/wahh</item>
	</style>

	<style name="AppTheme3" parent="AppTheme" >
		<item name="android:colorPrimaryDark">@color/colorEnabled</item>
		<item name="android:colorPrimary">@color/fgdfg</item>
		<item name="android:colorAccent">@color/colorEnabled</item>
		<item name="android:colorBackground">@color/colorPrimary</item>
		<item name="android:navigationBarColor">@color/fgdfg</item>
		<item name="android:colorForeground">@color/fgdfg</item>
		<item name="colorPrimary">@color/wahh</item>
	</style>

	<style name="AppTheme4" parent="AppTheme" >
		<item name="android:colorForeground">@color/colorDisabled</item>
		<item name="android:colorPrimary">@color/wahh</item>
		<item name="android:colorPrimaryDark">@color/wahh</item>
		<item name="android:navigationBarColor">@color/wahh</item>
		<item name="colorAccent">@color/wahh</item>
		<item name="colorPrimary">@color/fgdfg</item>
		<item name="android:colorBackground">@color/background_material_light</item>
	</style>

    <style name="AppTheme5" parent="AppTheme" >
		<item name="colorPrimary">@color/colorEnabled</item>
		<item name="android:colorBackground">@color/backgrsdl_light</item>
		<item name="colorAccent">@color/design_default_color_primary_dark</item>
	</style>

</resources>
